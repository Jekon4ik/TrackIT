{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "TrackIT": "DataSource=TrackIT.db"
  },
  "CacheSettings": {
    "TransactionsExpirationMinutes": 5,
    "CategoriesExpirationMinutes": 10, 
    "SlidingExpirationMinutes": 2,
    "EnableCaching": true
  },
  "ApiSettings": {
    "MaxTransactionsPerRequest": 50
  }
}
